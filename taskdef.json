{
    "family": "flask-app",
    "containerDefinitions": [
        {
            "image": "310444902345.dkr.ecr.us-west-2.amazonaws.com/getting-started/flask-app:v_%BUILD_NUMBER%",
            "name": "flask-app",
            "cpu": 10,
            "memory": 256,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 5000,
                    "hostPort": 80
                }
            ]
        }
    ]
}
