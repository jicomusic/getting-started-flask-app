{
    "family": "flask-app",
    "containerDefinitions": [
        {
            "image": "gettings-started/flask-app:v_%BUILD_NUMBER%",
            "name": "flask-app",
            "cpu": 10,
            "memory": 256,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 5000,
                    "hostPort": 80
                }
            ]
        }
    ]
}
